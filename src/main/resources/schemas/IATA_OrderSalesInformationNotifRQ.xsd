        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="ValueText" type="TextType">
        <xs:annotation>
          <xs:documentation>Value corresponding to the associated Keyword.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="LangCodeContentType">
    <xs:annotation>
      <xs:documentation> {codeListAgencyName}ISO{/codeListAgencyName} {codeListName}Language Code{/codeListName} {codeListIdentifier}6392{/codeListIdentifier} </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" />
